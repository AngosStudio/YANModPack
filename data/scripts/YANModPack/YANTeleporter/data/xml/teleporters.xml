<?xml version="1.0" encoding="UTF-8"?>
<teleporters xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="xsd/teleporters.xsd">
	<!-- ATTRIBUTES
		enabled			(required)	- true|false
		dialog_type		(required)	- NPC|COMMUNITY; NPC open npc html window, open COMMUNITY window
		html_folder		(required)	- html subfolder; which subfolder to use for yanbuffer htmls(data/scripts/YANModPack/YANBuffer/data/html/<html_folder>/)
		required_item	(optional)	- item id; default 0, 0 = no item required
	-->
	<voiced_teleporter enabled="false" dialog_type="COMMUNITY" html_folder="community">
		<solo_teleport_locations>
			<!-- teleports from solo_teleport_locations.xml -->
			<reference>S_LOC_01</reference>
			<reference>S_LOC_02</reference>
		</solo_teleport_locations>
		<party_teleport_locations>
			<!-- teleports from group_teleport_locations.xml -->
			<reference>PT_GRP_LOC_01</reference>
			<reference>PT_GRP_LOC_02</reference>
		</party_teleport_locations>
		<command_channel_teleport_locations>
			<!-- teleports from group_teleport_locations.xml -->
			<reference>CC_GRP_LOC_01</reference>
			<reference>CC_GRP_LOC_02</reference>
		</command_channel_teleport_locations>
	</voiced_teleporter>

	<npc_teleporters>
		<!-- ATTRIBUTES
			npc_id				(required)	- npc id; the npc who represents the buffer
			direct_first_talk	(required)	- true|false; true: yanbuffer main.html as first talk, false: html from data/html for first talk
			dialog_type			(required)	- NPC|COMMUNITY; NPC open npc html window, open COMMUNITY window
			html_folder			(required)	- html subfolder; which subfolder to use for yanbuffer htmls(data/scripts/YANModPack/YANBuffer/data/html/<html_folder>/)			
 		-->
		<npc_teleporter npc_id="1000007" direct_first_talk="true" dialog_type="NPC" html_folder="npc">
			<solo_teleport_locations>
				<!-- teleports from solo_teleport_locations.xml -->
				<reference>S_LOC_01</reference>
				<reference>S_LOC_02</reference>
			</solo_teleport_locations>
		<party_teleport_locations>
			<!-- teleports from group_teleport_locations.xml -->
			<reference>PT_GRP_LOC_01</reference>
			<reference>PT_GRP_LOC_02</reference>
		</party_teleport_locations>
		<command_channel_teleport_locations>
			<!-- teleports from group_teleport_locations.xml -->
			<reference>CC_GRP_LOC_01</reference>
			<reference>CC_GRP_LOC_02</reference>
		</command_channel_teleport_locations>
		</npc_teleporter>
	</npc_teleporters>
</teleporters>